{
  "name": "gcal-to-org",
  "description": "CLI for creating an Org Mode file based on meetings in a Google Calendar",
  "version": "1.0",
  "author": "Oliver Burn @oburn",
  "bin": {
    "gcal-to-org": "./bin/run-exec"
  },
  "pkg": {
    "scripts": "./lib/**/*.js"
  },
  "bugs": "https://github.com/oburn/gcal-to-org/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.54",
    "@types/server-destroy": "^1.0.1",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "mocha": "^8.3.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/oburn/gcal-to-org",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "bin": "gcal-to-org"
  },
  "repository": "oburn/gcal-to-org",
  "scripts": {
    "genbinary": "rm -rf lib && tsc -b && pkg --targets node14.4.0-macos-x64,node14.4.0-linux-x64 --out-dir dist .",
    "build": "tsc",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --require ts-node/register --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "date-fns": "^2.19.0",
    "googleapis": "^68.0.0",
    "open": "^8.0.4",
    "server-destroy": "^1.0.1",
    "tslib": "^1.14.1"
  }
}
