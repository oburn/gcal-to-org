    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.54",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "mocha": "^8.3.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
